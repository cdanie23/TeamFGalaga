<custom:Enemy1Sprite
    x:Class="Galaga.View.Sprites.EnemySprites.Enemy2Sprite"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:custom="using:Galaga.View.Sprites.EnemySprites"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d"
    d:DesignHeight="60"
    d:DesignWidth="50"
    Height="60"
    Width="50"
    >
    
    <UserControl.Resources>
        <SolidColorBrush x:Key="WingConnector2Color" Color="White"/>
    </UserControl.Resources>

    <Canvas Width="50" Height="60">
    <custom:Enemy1Sprite/>
    <Rectangle Height ="5" Width="6" Stroke ="{StaticResource WingConnector2Color}" StrokeThickness="3" Canvas.Left="6" Canvas.Top="19"/>
    <Rectangle Height ="5" Width="6" Stroke ="{StaticResource WingConnector2Color}" StrokeThickness="3" Canvas.Left="38" Canvas.Top="19"/>
    </Canvas>
</custom:Enemy1Sprite>
