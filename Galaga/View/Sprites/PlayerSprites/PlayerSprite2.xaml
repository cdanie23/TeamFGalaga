        <Line X1="33" Y1="46" X2="33" Y2="56" Stroke="{StaticResource RocketBoosterFlames}" StrokeThickness="4" />
        <Line X1="36" Y1="50" X2="36" Y2="56" Stroke="{StaticResource RocketBoosterFlames}" StrokeThickness="4" />

        <Line X1="2" Y1="14" X2="2" Y2="20" Stroke="{StaticResource GunTipColor}" StrokeThickness="4" />
        <Line X1="48" Y1="14" X2="48" Y2="20" Stroke="{StaticResource GunTipColor}" StrokeThickness="4" />

        <Line X1="25" X2="25" Y1="23" Y2="38" Stroke="{StaticResource CockPitColor}" StrokeThickness="6" />
    </Canvas>
