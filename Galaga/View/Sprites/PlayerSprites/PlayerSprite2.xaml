    <Canvas>
        <Line X1="25" Y1="58" X2="25" Y2="2" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
        <Line X1="21" Y1="54" X2="21" Y2="8" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
        <Line X1="18" Y1="56" X2="18" Y2="16" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
        <Line X1="14" Y1="56" X2="14" Y2="19" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
