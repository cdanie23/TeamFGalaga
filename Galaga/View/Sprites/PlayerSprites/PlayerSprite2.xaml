        <Line X1="44" Y1="52" X2="44" Y2="21" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
        <Line X1="48" Y1="54" X2="48" Y2="14" Stroke="{StaticResource MainColor }" StrokeThickness="4" />

        <Line X1="10" Y1="17" X2="10" Y2="10" Stroke="{StaticResource GunTipColor}" StrokeThickness="4" />
