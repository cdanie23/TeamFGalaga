        <Line X1="2" Y1="54" X2="2" Y2="14" Stroke="{StaticResource MainColor }" StrokeThickness="4" />

        <Line X1="29" Y1="54" X2="29" Y2="8" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
        <Line X1="33" Y1="56" X2="33" Y2="16" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
        <Line X1="36" Y1="56" X2="36" Y2="19" Stroke="{StaticResource MainColor }" StrokeThickness="4" />
